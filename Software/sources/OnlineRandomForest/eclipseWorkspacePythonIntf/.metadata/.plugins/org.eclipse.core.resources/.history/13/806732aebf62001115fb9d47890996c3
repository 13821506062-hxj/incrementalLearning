import numpy


def createNormDistData(samples, mean=(0,0), var=(1,1), rotation=0):
    mean = (1,2)
    cov =   [[var[0],-0],
             [-0,var[1]]]
    x1 = numpy.random.multivariate_normal(mean,cov,(samples))



